<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.google.android.gms.location.sample.locationupdates"
    android:sharedUserId="com.google.android.gms.location.sample.locationupdates" >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <!-- We use sharedUserId to apply the ACCESS_MOCK_LOCATION permission to the application APK. -->
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Base" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".SensorTest"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".CalcActivity"
            android:label="@string/title_activity_map" >
        </activity>
        <activity
            android:name=".Camera_Intent"
            android:label="@string/title_activity_camera__intent" >
        </activity>
        <activity
            android:name=".gps_activity"
            android:label="@string/title_activity_gps_activity" >
        </activity>
        <activity
            android:name=".CRUD"
            android:label="@string/title_activity_crud" >
        </activity>
        <activity
            android:name=".GPXData"
            android:label="@string/title_activity_gpxdata" >
        </activity>
        <activity
            android:name=".StartActivity"
            android:label="@string/title_activity_start" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Routen"
            android:label="@string/title_activity_routen" >
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login" >
        </activity>
    </application>

</manifest>
